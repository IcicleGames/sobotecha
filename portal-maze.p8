pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- portal-maze
-- by sobotecha

--------------------------------
------ initalize startup -------
--------------------------------
function _init()
 reset()
end

--------------------------------
---- reset player position -----
--------------------------------
function reset()
 player={
  x=8, y=8,
  isingate=false, iswalking=false,
  isleft=false, step=0, sprite=1
 }
 portal={
  x=flr(rnd(14))*8, y=flr(rnd(14))*8,
  sprite=2, flag=1, set=true
 }
end

--------------------------------
----- 60 fps update check ------
--------------------------------
function _update60()
 -- if the player touches the
 -- gate wait for button
 if not player.ingate then
  move_player()
 else
 -- try_again()
 next_level()
 end
 
 -- player running animation
 if player.iswalking then
  step+=0.3
 else
  step=0
 end
 
 -- check if portal is in a wall
 if (check_tile(portal.x,portal.y,0)) then
  portal.x=flr(rnd(14))*8
  portal.y=flr(rnd(14))*8
 end
 
 -- check player against gate
 if player.x==portal.x and player.y==portal.y then
  player.ingate=true
 end
 
end

--------------------------------
------ player controller -------
--------------------------------
function move_player()
 -- getting old position
 local old_x=player.x
 local old_y=player.y
 
 -- left control
 if btn(0) then
  player.x-=1
  player.isleft=true
  if player.x%10==0 then
   sfx(0)
  end
 end
 
 -- right control
 if btn(1) then
  player.x+=1
  player.isleft=false
  if player.x%10==0 then
   sfx(0)
  end
 end
 
 -- up control
 if btn(2) then
  player.y-=1
  if player.y%10==0 then
   sfx(0)
  end
 end
 
 -- down control
 if btn(3) then
  player.y+=1
  if player.y%10==0 then
   sfx(0)
  end
 end
 
 -- wall collision
 if wall_collision(player.x,player.y) then
  player.x=old_x
  player.y=old_y
 end
 
 -- gate collision
 if check_tile(player.x+3,player.y+3,1) then
  player.ingate=true
 end
 
 -- walking animation
 if player.x!=old_x or
    player.y!=old_y then
  player.iswalking=true
 else
  player.iswalking=false
 end
end

-- check player against map
function wall_collision(x,y)
 if (check_tile(player.x,player.y,0)) return true
 if (check_tile(player.x+7,player.y,0)) return true
 if (check_tile(player.x,player.y+7,0)) return true
 if (check_tile(player.x+7,player.y+7,0)) return true
 return false
end

-- check wall flag
function check_tile(x,y,flag)
 local tile_x=x/8
 local tile_y=y/8
 local tile=mget(tile_x,tile_y)
 return fget(tile,flag)
end

-- try again button
function try_again()
 if btn(4) or btn(5) then
  reset()
 end
end

-- next level
function next_level()
 player.isingate=false
 reset()
end

--------------------------------
----- drawing the screen -------
--------------------------------
function _draw()
 -- clear the screen
 cls()
 -- draw the background
 rectfill(0,0,127,127,5)
 -- draw the map
 map()
 -- draw another portal
 spr(portal.sprite,portal.x,portal.y)
  -- draw the player
 spr(player.sprite,player.x,player.y-flr(step%2),1,1,player.isleft)
 -- win message
 if player.isingate then
  rectfill(0,60,127,75,1)
  print("next level",45,62,8)
  print("press button to restart",15,69,4)
 end
end
